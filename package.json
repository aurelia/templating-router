{
  "name": "aurelia-templating-router",
  "version": "1.3.3",
  "description": "An implementation of the RouteLoader interface for use with the router module. Also contains a custom element that allows the templating engine to display the current route.",
  "keywords": [
    "aurelia",
    "templating",
    "html"
  ],
  "homepage": "http://aurelia.io",
  "bugs": {
    "url": "https://github.com/aurelia/templating-router/issues"
  },
  "license": "MIT",
  "author": "Rob Eisenberg <rob@bluespire.com> (http://robeisenberg.com/)",
  "main": "dist/commonjs/aurelia-templating-router.js",
  "module": "dist/es2015/aurelia-templating-router.js",
  "typings": "dist/aurelia-templating-router.d.ts",
  "repository": {
    "type": "git",
    "url": "http://github.com/aurelia/templating-router"
  },
  "scripts": {
    "start": "npm run dev -- --format es2015",
    "dev": "npm run build -- --dev",
    "prebuild": "rimraf dist",
    "build": "cross-env TS_NODE_PROJECT=build/tsconfig.json ts-node build/build.ts",
    "build:dts": "dts-bundle-generator",
    "karma": "cross-env TS_NODE_PROJECT=test/.karma/tsconfig.json karma start test/.karma/karma.conf.ts",
    "test": "npm run karma -- --unit --integration --single-run --browsers ChromeHeadlessOpt",
    "test:watch": "npm run karma -- --unit --integration --coverage --browsers ChromeHeadlessOpt",
    "test:debugger": "npm run karma -- --unit --integration --coverage --browsers ChromeDebugging",
    "changelog": "cross-env TS_NODE_PROJECT=build/tsconfig.json ts-node build/changelog.ts",
    "lint": "tslint -c tslint.json '{src,test}/**/*.ts' --force"
  },
  "jspm": {
    "registry": "npm",
    "jspmPackage": true,
    "main": "aurelia-templating-router",
    "format": "amd",
    "directories": {
      "dist": "dist/amd"
    },
    "peerDependencies": {
      "aurelia-binding": "^2.0.0",
      "aurelia-dependency-injection": "^1.0.0",
      "aurelia-logging": "^1.0.0",
      "aurelia-metadata": "^1.0.0",
      "aurelia-pal": "^1.3.0",
      "aurelia-path": "^1.0.0",
      "aurelia-router": "^1.5.0",
      "aurelia-templating": "^1.8.1"
    },
    "dependencies": {
      "aurelia-binding": "^2.0.0",
      "aurelia-dependency-injection": "^1.0.0",
      "aurelia-logging": "^1.0.0",
      "aurelia-metadata": "^1.0.0",
      "aurelia-pal": "^1.3.0",
      "aurelia-path": "^1.0.0",
      "aurelia-router": "^1.5.0",
      "aurelia-templating": "^1.8.1"
    }
  },
  "dependencies": {
    "aurelia-binding": "^2.0.0",
    "aurelia-dependency-injection": "^1.0.0",
    "aurelia-logging": "^1.0.0",
    "aurelia-metadata": "^1.0.0",
    "aurelia-pal": "^1.3.0",
    "aurelia-path": "^1.0.0",
    "aurelia-router": "git+https://github.com/aurelia/router.git",
    "aurelia-templating": "^1.8.1"
  },
  "devDependencies": {
    "@types/jasmine": "^2.8.8",
    "@types/karma": "^3.0.0",
    "@types/node": "^10.11.3",
    "@types/rimraf": "^2.0.2",
    "@types/webpack": "^4.4.13",
    "@types/webpack-env": "^1.13.6",
    "@types/yargs": "^12.0.1",
    "aurelia-bootstrapper": "^2.3.0",
    "aurelia-framework": "^1.3.0",
    "aurelia-history-browser": "^1.2.0",
    "aurelia-loader-nodejs": "^1.0.1",
    "aurelia-loader-webpack": "^2.2.1",
    "aurelia-logging-console": "^1.0.0",
    "aurelia-pal-browser": "^1.8.0",
    "aurelia-route-recognizer": "^1.3.1",
    "aurelia-templating-binding": "^1.5.1",
    "aurelia-templating-resources": "^1.7.1",
    "aurelia-testing": "^1.0.0-beta.4.0.0",
    "aurelia-webpack-plugin": "^3.0.0",
    "conventional-changelog": "^3.0.5",
    "cross-env": "^5.2.0",
    "dts-bundle-generator": "^1.6.1",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jasmine-core": "^2.4.1",
    "karma": "^3.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-jasmine": "^1.1.2",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^3.0.5",
    "rimraf": "^2.6.2",
    "rollup": "^0.66.6",
    "rollup-plugin-typescript2": "^0.17.2",
    "ts-loader": "^4.5.0",
    "ts-node": "^7.0.1",
    "tslib": "^1.9.3",
    "tslint": "^5.11.0",
    "typescript": "^3.1.1",
    "webpack": "^4.24.0",
    "yargs": "^4.8.1"
  },
  "peerDependencies": {
    "aurelia-router": "^1.5.0"
  }
}
